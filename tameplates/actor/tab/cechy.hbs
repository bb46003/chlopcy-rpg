
    <div class="manin">
        <div class="lewa">
           
                {{#if system.pod_wplywem_pylku}}
                  <div class="rzuty-label-pod-wpÅ‚ywem">
                    <label class="KB-2k10">{{localize "chlopcy.KB_z_pylkiem"}}</label>
                    <label class="KM-pk10">{{localize "chlopcy.KM.K10"}}</label>
                    <label class="KM-pk8">{{localize "chlopcy.KM.K8"}}</label>
                    <label class="KM-pk6">{{localize "chlopcy.KM.K6"}}</label>
                    <label class="KM-pk4">{{localize "chlopcy.KM.K4"}}</label>
                    <label class="KM-pk3">{{localize "chlopcy.KM.K3"}}</label>
                    <label class="KM-pk2">{{localize "chlopcy.KM.K2"}}</label>
                    
             
                {{else}}
                  <div class="rzuty-label">
                    <label class="KB">{{localize "chlopcy.KB"}}</label>
                    <label class="KM-20">{{localize "chlopcy.KM.K20"}}</label>
                    <label class="KM-12">{{localize "chlopcy.KM.K12"}}</label>
                    <label class="KM-10">{{localize "chlopcy.KM.K10"}}</label>
                    <label class="KM-8">{{localize "chlopcy.KM.K8"}}</label>
                    <label class="KM-6">{{localize "chlopcy.KM.K6"}}</label>
                    <label class="KM-4">{{localize "chlopcy.KM.K4"}}</label>
                {{/if}}
            </div>
            <div class="cechy">
                <div class="cechy-label">
                    <a class="cecha">{{localize "chlopcy.brutal"}}</a>
                    <a class="cecha">{{localize "chlopcy.spryciarz"}}</a>
                    <a class="cecha">{{localize "chlopcy.fachura"}}</a>
                    <a class="cecha">{{localize "chlopcy.bajerant"}}</a>
                    <a class="cecha">{{localize "chlopcy.orient"}}</a>
                    <a class="cecha">{{localize "chlopcy.bajker"}}</a>
                    <a class="cecha">{{localize "chlopcy.farciarz"}}</a>
                </div>
                <div class="cechy-boxy">
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.brutal.wartosc cecha="brutal"}}
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.spryciarz.wartosc cecha="spryciarz"}}
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.fachura.wartosc cecha="fachura"}}
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.bajerant.wartosc cecha="bajerant"}}
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.orient.wartosc cecha="orient"}}
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.bajker.wartosc cecha="bajker"}}
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.farciarz.wartosc cecha="farciarz"}}
                </div>
            </div>
        </div>
        <div class="prawa">
          
            <div class="label-tagi">
                <label class="tagi">{{localize "chlopcy.tagi.etykieta"}}</label>
                <label class="label-poziom-tagu">{{localize "chlopcy.przerzut_KB"}}</label>
                <label class="label-poziom-tagu">{{localize "chlopcy.przerzut_KM"}}</label>
                <label class="label-poziom-tagu">{{localize "chlopcy.dodatkowa_KM"}}</label>
                <label class="label-poziom-tagu">{{localize "chlopcy.plus_minus_1"}}</label>
                <label class="label-poziom-tagu">{{localize "chlopcy.dodatkowy_bonus"}}</label>
            </div>
          
        <div class="tagi-dane">
            <div class="nazwy-tagow">
                <input type="text" class="tag-input" name="system.tagi.brutal.opis" value="{{system.tagi.brutal.opis}}" {{#if (eq system.tagi.brutal.opis "")}} data-tooltip="{{localize "chlopcy.tagi.brutal"}}"{{/if}}>
                <input type="text" class="tag-input" name="system.tagi.spryciarz.opis" value="{{system.tagi.spryciarz.opis}}" {{#if (eq system.tagi.spryciarz.opis "")}}data-tooltip="{{localize "chlopcy.tagi.spryciarz"}}"{{/if}}>
                <input type="text" class="tag-input" name="system.tagi.fachura.opis" value="{{system.tagi.fachura.opis}}" {{#if (eq system.tagi.fachura.opis "")}}data-tooltip="{{localize "chlopcy.tagi.fachura"}}"{{/if}}>
                <input type="text" class="tag-input" name="system.tagi.bajerant.opis" value="{{system.tagi.bajerant.opis}}" {{#if (eq system.tagi.bajerant.opis "")}}data-tooltip="{{localize "chlopcy.tagi.bajerant"}}"{{/if}}>
                <input type="text" class="tag-input" name="system.tagi.orient.opis" value="{{system.tagi.orient.opis}}" {{#if (eq system.tagi.orient.opis "")}}data-tooltip="{{localize "chlopcy.tagi.orient"}}"{{/if}}>
                <select name="system.tagi.bajker.opis"  class="tag-input-bajker">
                    <option value="" data-tooltip="{{localize "chlopcy.tagi.bajker"}}"></option>
                    {{selectOptions bajker selected=system.tagi.bajker.opis localize=true}}

                </select>
                <input type="text" class="tag-input" name="system.tagi.farciarz.opis" value="{{system.tagi.farciarz.opis}}" {{#if (eq system.tagi.farciarz.opis "")}}data-tooltip="{{localize "chlopcy.tagi.farciarz"}}"{{/if}}>
            </div>
            <div class="tagi-boxy">
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.tagi.brutal.wartosc cecha="brutal"}}
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.tagi.spryciarz.wartosc cecha="spryciarz"}}
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.tagi.fachura.wartosc cecha="fachura"}}
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.tagi.bajerant.wartosc cecha="bajerant"}}
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.tagi.orient.wartosc cecha="orient"}}
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.tagi.bajker.wartosc cecha="bajker"}}
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.tagi.farciarz.wartosc cecha="farciarz"}}

            </div>
            </div>
        </div>
        
    </div>
