
    <div class="manin">
        <div class="lewa">
            <div class="rzuty-label">
                {{#if system.pod_wplywem_pylku}}
                    <label class="KB-2k10">{{localize "chlopcy.KB_z_py≈Çkiem"}}</label>
                    <label class="KM-pk10">{{localize "chlopcy.KM.K10"}}</label>
                    <label class="KM-pk8">{{localize "chlopcy.KM.K8"}}</label>
                    <label class="KM-pk6">{{localize "chlopcy.KM.K6"}}</label>
                    <label class="KM-pk4">{{localize "chlopcy.KM.K4"}}</label>
                    <label class="KM-pk3">{{localize "chlopcy.KM.K3"}}</label>
                    <label class="KM-pk2">{{localize "chlopcy.KM.K2"}}</label>
                    
             
                {{else}}
                    <label class="KB">{{localize "chlopcy.KB"}}</label>
                    <label class="KM-20">{{localize "chlopcy.KM.K20"}}</label>
                    <label class="KM-12">{{localize "chlopcy.KM.K12"}}</label>
                    <label class="KM-10">{{localize "chlopcy.KM.K10"}}</label>
                    <label class="KM-8">{{localize "chlopcy.KM.K8"}}</label>
                    <label class="KM-6">{{localize "chlopcy.KM.K6"}}</label>
                    <label class="KM-4">{{localize "chlopcy.KM.K4"}}</label>
                {{/if}}
            </div>
            <div class="cechy">
                <div class="cechy-label">
                    <label class="cecha">{{localize "chlopcy.brutal"}}</label>
                    <label class="cecha">{{localize "chlopcy.spryciarz"}}</label>
                    <label class="cecha">{{localize "chlopcy.fachura"}}</label>
                    <label class="cecha">{{localize "chlopcy.bajerant"}}</label>
                    <label class="cecha">{{localize "chlopcy.orient"}}</label>
                    <label class="cecha">{{localize "chlopcy.bajker"}}</label>
                    <label class="cecha">{{localize "chlopcy.farciarz"}}</label>
                </div>
                <div class="cechy-boxy">
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.brutal cecha="brutal"}}
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.spryciarz cecha="spryciarz"}}
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.fachura cecha="fachura"}}
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.bajerant cecha="bajerant"}}
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.orient cecha="orient"}}
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.bajker cecha="bajker"}}
                    {{> "systems/chlopcy/tameplates/actor/tab/cechy-check-box.hbs" wartosc = system.cechy.farciarz cecha="farciarz"}}
                </div>
            </div>
        </div>
        <div class="prawa">
          
            <div class="label-tagi">
                <label class="tagi">{{localize "chlopcy.tagi.etykieta"}}</label>
                <label class="label-poziom-tagu">{{localize "chlopcy.przerzut_KB"}}</label>
                <label class="label-poziom-tagu">{{localize "chlopcy.przerzut_KM"}}</label>
                <label class="label-poziom-tagu">{{localize "chlopcy.dodatkowa_KM"}}</label>
                <label class="label-poziom-tagu">{{localize "chlopcy.plus_minus_1"}}</label>
                <label class="label-poziom-tagu">{{localize "chlopcy.dodatkowy_bonus"}}</label>
            </div>
          
        <div class="tagi-dane">
            <div class="nazwy-tagow">
                <input type="text" class="tag-input" name="system.tagi.brutal.opis" value="{{system.tagi.brutal.opis}}" data-tooltip="{{localize "chlopcy.tagi.brutal"}}">
                <input type="text" class="tag-input" name="system.tagi.spryciarz.opis" value="{{system.tagi.spryciarz.opis}}" data-tooltip="{{localize "chlopcy.tagi.spryciarz"}}">
                <input type="text" class="tag-input" name="system.tagi.fachura.opis" value="{{system.tagi.fachura.opis}}" data-tooltip="{{localize "chlopcy.tagi.fachura"}}">
                <input type="text" class="tag-input" name="system.tagi.bajerant.opis" value="{{system.tagi.bajerant.opis}}" data-tooltip="{{localize "chlopcy.tagi.bajerant"}}">
                <input type="text" class="tag-input" name="system.tagi.orient.opis" value="{{system.tagi.orient.opis}}" data-tooltip="{{localize "chlopcy.tagi.orient"}}">
                <select name="system.tagi.bajker.opis"  class="tag-input-bajker">
                    <option value="" data-tooltip="{{localize "chlopcy.tagi.bajker"}}"></option>
                    {{selectOptions bajker selected=system.tagi.bajker.opis localize=true}}

                </select>
                <input type="text" class="tag-input" name="system.tagi.farciarz.opis" value="{{system.tagi.brutal.opis}}" data-tooltip="{{localize "chlopcy.tagi.farciarz"}}">
            </div>
            <div class="tagi-boxy">
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.cechy.brutal cecha="brutal"}}
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.cechy.spryciarz cecha="spryciarz"}}
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.cechy.fachura cecha="fachura"}}
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.cechy.bajerant cecha="bajerant"}}
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.cechy.orient cecha="orient"}}
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.cechy.bajker cecha="bajker"}}
                {{> "systems/chlopcy/tameplates/actor/tab/tagi-check-box.hbs" wartosc = system.cechy.farciarz cecha="farciarz"}}

            </div>
            </div>
        </div>
        
    </div>
